{
    "name": "hono-oauth-playground",
    "type": "module",
    "version": "0.1.0",
    "private": true,
    "workspaces": [
        "apps/*"
    ],
    "scripts": {
        "dev": "pnpm -r dev",
        "lint": "biome lint .",
        "lint:fix": "biome lint . --write",
        "format": "prettier . --write",
        "prepare": "lefthook install"
    },
    "dependencies": {
        "@hono/node-server": "^1.14.3",
        "hono": "4.7.10",
        "honox": "0.1.40",
        "nanoid": "^5.1.5"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@cloudflare/workers-types": "^4.20250214.0",
        "@hono/vite-build": "1.3.0",
        "@hono/vite-dev-server": "0.18.2",
        "@ianvs/prettier-plugin-sort-imports": "4.4.1",
        "@tailwindcss/postcss": "4.1.7",
        "@tailwindcss/vite": "4.0.9",
        "@types/node": "20.17.50",
        "lefthook": "1.11.13",
        "prettier": "3.5.3",
        "prettier-plugin-tailwindcss": "0.6.11",
        "tailwindcss": "4.1.7",
        "typescript": "5.5.4",
        "vite": "6.1.1",
        "wrangler": "^4.4.0"
    },
    "packageManager": "pnpm@10.11.0",
    "pnpm": {
        "onlyBuiltDependencies": [
            "@biomejs/biome",
            "@tailwindcss/oxide",
            "core-js",
            "esbuild",
            "lefthook",
            "sharp",
            "workerd"
        ]
    }
